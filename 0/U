FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (680 0 0);

boundaryField
{
    inlet
    {
        type            freestreamVelocity;
        freestreamValue      $internalField;
    }

    outlet
    {
        type            inletOutlet;
	inletValue	$internalField;
	value		$internalField;
    }

    airfoil
    {
        type            slip;
    }

    frontAndBack
    {
        type            empty;
    }
}

